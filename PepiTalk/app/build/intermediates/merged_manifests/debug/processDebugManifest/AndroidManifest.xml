<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.pepitalk"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="30"
        android:targetSdkVersion="34" />

    <permission
        android:name="com.example.pepitalk.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION"
        android:protectionLevel="signature" />

    <uses-permission android:name="com.example.pepitalk.DYNAMIC_RECEIVER_NOT_EXPORTED_PERMISSION" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:debuggable="true"
        android:extractNativeLibs="false"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.PepiTalk" >
        <activity
            android:name="com.example.pepitalk.VerOfertas"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.pepitalk.Grupo"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Login"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Oferta"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Registro"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Reunion"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Ruta"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.VerOferta"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Perfil"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.VerReunion"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.VerReuniones"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.VerGrupo"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.MainActivity"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.MenuCliente"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.MenuTraductor"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.ActualizarCuenta"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.ActualizarGrupo"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.ActualizarReunion"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.AceptarTraductor"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.CrearGrupo"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.CrearOferta"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.CrearReunion"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.Calificar"
            android:exported="false" >
        </activity>
        <activity
            android:name="com.example.pepitalk.VerGrupos"
            android:exported="false" >
        </activity>

        <provider
            android:name="androidx.startup.InitializationProvider"
            android:authorities="com.example.pepitalk.androidx-startup"
            android:exported="false" >
            <meta-data
                android:name="androidx.emoji2.text.EmojiCompatInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.lifecycle.ProcessLifecycleInitializer"
                android:value="androidx.startup" />
            <meta-data
                android:name="androidx.profileinstaller.ProfileInstallerInitializer"
                android:value="androidx.startup" />
        </provider>

        <receiver
            android:name="androidx.profileinstaller.ProfileInstallReceiver"
            android:directBootAware="false"
            android:enabled="true"
            android:exported="true"
            android:permission="android.permission.DUMP" >
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.INSTALL_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SKIP_FILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.SAVE_PROFILE" />
            </intent-filter>
            <intent-filter>
                <action android:name="androidx.profileinstaller.action.BENCHMARK_OPERATION" />
            </intent-filter>
        </receiver>
    </application>

</manifest>